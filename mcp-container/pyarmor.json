{
  "project": {
    "name": "contpaqi-ai-bridge",
    "description": "ContPAQi AI Bridge - Invoice Processing Engine",
    "version": "1.0.0"
  },
  "pyarmor": {
    "version": "8.x",
    "license": "trial"
  },
  "obfuscation": {
    "src": "src",
    "output": "dist",
    "entry": "main.py",
    "recursive": true,
    "includes": [
      "*.py"
    ],
    "excludes": [
      "__pycache__",
      "*.pyc",
      "*.pyo",
      "test_*.py",
      "*_test.py",
      "tests/"
    ]
  },
  "settings": {
    "python_version": "3.9",
    "platform": "linux",
    "restrict_mode": 2,
    "obf_code": 2,
    "obf_module": 1,
    "wrap_mode": 1,
    "advanced_mode": 0,
    "enable_suffix": 0
  },
  "string_encryption": {
    "enabled": true,
    "description": "Encrypt sensitive strings in source code to prevent extraction",
    "selective": true,
    "patterns": [
      "api_key",
      "api_secret",
      "password",
      "secret",
      "token",
      "credential",
      "connection_string",
      "database_url",
      "redis_url",
      "endpoint",
      "base_url",
      "host",
      "contpaqi",
      "license"
    ],
    "exclude_patterns": [
      "DEBUG",
      "INFO",
      "WARNING",
      "ERROR",
      "CRITICAL",
      "__name__",
      "__main__",
      "utf-8",
      "application/json"
    ],
    "notes": {
      "runtime_visibility": "Encrypted strings are decrypted at runtime and may be visible in memory",
      "performance": "String encryption adds minimal runtime overhead",
      "pyarmor_8": "PyArmor 8.x includes string encryption in obf_code level 2"
    }
  },
  "runtime": {
    "include_runtime": true,
    "runtime_path": "pytransform"
  },
  "targets": {
    "primary": [
      "src/main.py",
      "src/inference.py"
    ],
    "models": [
      "src/models/tatr.py",
      "src/models/layoutlm.py",
      "src/models/validators.py",
      "src/models/schemas.py"
    ],
    "utils": [
      "src/utils/ocr.py"
    ]
  },
  "bootstrap": {
    "entry_script": "src/main.py",
    "disable_restrict_mode": false
  }
}
